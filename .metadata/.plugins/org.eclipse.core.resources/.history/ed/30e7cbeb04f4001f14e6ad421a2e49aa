package ch02.sec1;

public class BooleanExample {
	public static void main (String[] args) {
		boolean stop = true;
		
		stop = false;
		
		/* > : 크다
		   >= : 크거나 같다
		   < : 작다
		   <= : 작거나 같다
		   == : 같다
		   != : 같지 않다
		   ! : 부정
		   && : 그리고(AND)
		   || : 혹은, 또는(OR)
		 */
		
		
		boolean check = (5>2) && (3>1);
		System.out.println(check);
		
	}
}
